s-serif;    \n    padding: 0;\n    margin: 0;\n    max-width: 1600px;\n    // position: relative;\n    background: url(../img/background_065.jpeg);\n    background-size: cover;\n    background-attachment: fixed;\n}\n.header {\n    display: flex;\n    position: relative;\n    justify-content: space-between;\n    padding: 30px 200px 30px 200px;\n    align-items: center;\n    font-size: 50px;\n    background-color: rgba(255, 255, 255, 0.1);\n}\n.header-logo-link {\n    color:white;\n}\n.createVisit-form {\n    padding: 10px 30px;\n    display: grid;\n    grid-gap: 10px;\n    // justify-items: center;\n    // flex-wrap: wrap;\n}\n.modal-dialog {\n    position: fixed;\n            top: 0;\n            left: 0;\n            width: 100%;\n            height: 100%;\n            background: #000;\n            opacity: 0.7;\n            z-index: 5;\n}\n.modal-content {\n    min-width: 300px;\n    min-height: 200px;\n    position: absolute;\n    background-color: white;\n    z-index: 1000;\n    top:50%;\n    left:50%;\n    transform:translate(-50%, -50%);\n    border-radius: 5px;\n    border: 1px groove black;\n}\n.modal-header {\n    text-align: center;\n}\n.btn-close {\n    position: absolute;\n    right: 2px;\n    top: 5px;\n    width: 32px;\n    height: 32px;\n    opacity: 0.3;\n}\n.btn-close:hover {\n    opacity: 1;\n    }\n    .btn-close:before, .btn-close:after {\n    position: absolute;\n    left: 15px;\n    content: ' ';\n    height: 15px;\n    width: 2px;\n    background-color: #333;\n    }\n    .btn-close:before {\n    transform: rotate(45deg);\n    }\n    .btn-close:after {\n    transform: rotate(-45deg);\n    }\n.login-form {\n    // display: flex;\n    // flex-wrap: wrap;\n    display: grid;\n    grid-gap: 10px;\n    padding: 10px 30px;\n}\n.input-label {\n    display: grid;\n}\n.login-form:last-child {\n    margin-top: 10px;\n}\n.no-visits-notice {\n    margin: 10% auto;\n    color: white;\n    font-size: 20px;\n}\n.main_container {\n    padding: 30px 200px 30px 200px;\n}\n.visits {\n    display: flex;\n    flex-wrap: wrap;\n    text-align: center;\n    justify-content: center;\n    // margin: 0 auto;\n    // justify-items: center;\n    \n}\n.visit-card {\n    background-color: rgba(255, 255, 255, 0.1);\n    // background-color: cadetblue;\n    margin: 5px;\n    width: 300px;\n    min-height: 200px;\n    color: white;\n}\n.hidden {\n    display: none;\n}\n.show-more-btn {\n    margin-top: 20px;\n}\n.show-less-btn {\n    margin-top: 20px;\n    margin-bottom: 10px;\n}\nbutton {\n    display: inline-block;\n    color: black;\n    // font-weight: 700;\n    text-decoration: none;\n    user-select: none;\n    padding: .5em 2em;\n    outline: none;\n    border: 2px solid;\n    border-radius: 5px;\n    transition: 0.2s;\n    cursor: pointer;\n}\nbutton:hover { \n    background: rgba(255,255,255,.2);\n    color: white;\n}\nbutton:active {\n    background: white;\n}\n.filter-container {\n    display: flex;\n    justify-content: space-between;\n    background-color: rgba(255, 255, 255, 0.1);\n    color: white;\n    width: 600px;\n    margin: 15px auto;\n    padding: 10px;\n}\n"},"lineCount":null}},"error":null,"hash":"8cc26f572c3a8af7d7d6f5b8163c6040","cacheData":{"env":{}}}